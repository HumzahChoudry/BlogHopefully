@helper Tags(System.Web.Mvc.HtmlHelper htmlHelper,
    IList<BlogHopefully.Core.Objects.Tag> tags)
{
foreach (var tag in tags)
{
        <div class="tags-div">
            @BlogHopefully.ActionLinkExtensions.TagLink(htmlHelper, tag)
        </div>
}
}

